<ul class="list-unstyled grid mt-5" style="grid-auto-rows: 1fr;" data-controller="insert-list">
  <% @lists.each do |list| %>
    <%= render "list_card", list: list %>
  <% end %>
  <li class="g-col-6 g-col-lg-4 g-col-xl-3" data-insert-list-target="last">
    <%= render "new_list_card" %>
  </li>
</ul>
